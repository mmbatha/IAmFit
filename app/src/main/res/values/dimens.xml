<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Define standard dimensions to comply with Holo-style grids and rhythm. -->

    <dimen name="margin_tiny" tools:keep="@dimen/margin_tiny">4dp</dimen>
    <dimen name="margin_small" tools:keep="@dimen/margin_small">8dp</dimen>
    <dimen name="margin_medium" tools:keep="@dimen/margin_medium">16dp</dimen>
    <dimen name="margin_large" tools:keep="@dimen/margin_large">32dp</dimen>
    <dimen name="margin_huge" tools:keep="@dimen/margin_huge">64dp</dimen>

    <!-- Semantic definitions -->

    <dimen name="horizontal_page_margin" tools:keep="@dimen/horizontal_page_margin">@dimen/margin_medium</dimen>
    <dimen name="vertical_page_margin" tools:keep="@dimen/vertical_page_margin">@dimen/margin_medium</dimen>
    <!-- Default screen margins, per the Android Design guidelines. -->
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="appbar_padding">16dp</dimen>
    <dimen name="fab_margin">16dp</dimen>
    <dimen name="appbar_padding_top">8dp</dimen>
</resources>
